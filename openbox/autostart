#enable compositing
xcompmgr&

#disable screen dimming
xset s off -dpms

#start ssh-agent
eval `ssh-agent`

#set the background
#eval `cat ~/.fehbg`
feh --bg-scale ~/Pictures/bg/$(ls ~/Pictures/bg | sort -R | head -n 1)

#open the panel
tint2&

#give me a volume control
volumeicon&

#open up mocicon
mocicon&

#set desktops vertical
setlayout 1 1 4 0

#gimme that dank conky
conky&

#a starting terminal is nice too
urxvt&
